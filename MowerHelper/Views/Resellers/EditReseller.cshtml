            <script type="text/javascript">
                $(function () {
                    $("#txt_CPhone1").keyup(function () {
                        if ($(this).val().length == 3) {
                            $("#txt_CPhone2").focus();
                        }
                    });
                    $("#txt_CPhone2").keyup(function () {
                        if ($(this).val().length == 3) {
                            $("#txt_CPhone3").focus();
                        }
                    });
                    $("#txt_WPhone1").keyup(function () {
                        if ($(this).val().length == 3) {
                            $("#txt_WPhone2").focus();
                        }
                    });
                    $("#txt_WPhone2").keyup(function () {
                        if ($(this).val().length == 3) {
                            $("#txt_WPhone3").focus();
                        }
                    });

                    $("#txtZip").keyup(function () {
                        if ($(this).val().length == 5) {
                            $("#DDLState").focus();
                        }
                    });

                });
    </script>
