                        document.getElementById('txtDob').focus();
                        return false;
                    }
                }

                var struname = trim(document.getElementById('txtUserName').value);
                document.getElementById('txtUserName').value = struname;
                if (document.getElementById('txtUserName').value == '') {
                    ValidateAlert('tblErrResp', 'lblErruname', 'tdErruname', 'tduname', "Please enter username")
