                                        @Html.TextBox("txtFName", Model[0].FirstName, new { @class = "textfield", MaxLength = "50", style = "width:128px;" })

                                        @Html.TextBox("txtMName", Model[0].MI, new { @class = "textfield", MaxLength = "1", style = "width:15px;" })

                                        @Html.TextBox("txtLName", Model[0].LastName, new { @class = "textfield", MaxLength = "50", style = "width:128px;" })
