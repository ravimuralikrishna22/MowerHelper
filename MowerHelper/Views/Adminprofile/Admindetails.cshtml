                        }
                        if (MyId > 31 || MyId == 0) {
                            //Glb_ShowAlert('Invalid date of birth!!! date should be entered in between 1 to 31');
                            ValidateAlert('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', 'Please enter a valid day');
                            document.getElementById('txtDob').focus();
                            return false
                        }
                        else {
                            ValidateAlertClear('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', '#FFFFFF');
                        }
                        if (myDat > CurDt) {
                            //Glb_ShowAlert('Invalid date of birth!!! should be less than current date');
                            ValidateAlert('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', 'Invalid date of birth!!! should be less than current date');
                            document.getElementById('txtDob').focus();
                            return false;
                        }
                        else {
                            ValidateAlertClear('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', '#FFFFFF');
                        }
                        if (Monthid == 2 && MyId == 29 && (yearid % 4 != 0 || yearid % 100 == 0 && yearid % 400 != 0)) {
                            ValidateAlert('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', 'Please enter a valid day');
                            document.getElementById('txtDob').focus();
                            return false;
                        }
                        else {
                            ValidateAlertClear('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', '#FFFFFF');
                        }
                        if (((Monthid == 4 || Monthid == 6 || Monthid == 9 || Monthid == 11) && MyId == 31) || (Monthid == 2 && (MyId == 30 || MyId == 31))) {
                            ValidateAlert('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', 'Please enter a valid day');
                            document.getElementById('txtDob').focus();
                            return false;
                        }
                        else {
                            ValidateAlertClear('tblErrResp', 'lblErrDOB', 'tdErrDob', 'tdEDob', '#FFFFFF');
                        }
                    }

                }
                if (document.getElementById('txtDob').value != '' && document.getElementById('txtDob').value != 'mm/dd/yyyy') {
                    var PtDOB = new Date(document.getElementById('txtDob').value);
                    var CurDt = new Date()
