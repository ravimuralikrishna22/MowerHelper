                                        <strong>Address 1 :</strong>
                                    </th>
                                    <td style="width: 81%; height: 30px;">
                                        @Html.TextBox("txt_Address1",  Model[0].Address1, new { @class = "textfield", MaxLength = "50", style = "width:200px", autocomplete = "off" })
                                    </td>
                                </tr>
                                <tr class="nav_blue_color" height="30">
                                    <th align="right" style="width: 19%">
                                        <strong>Address 2 :</strong>
                                    </th>
