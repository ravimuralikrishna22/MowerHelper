@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
  @*  <meta name="viewport" content="width=device-width" />*@
   <title>Mower Helper-Find an Mower Helper</title>
    <script src="~/Scripts/Expire.js" type="text/javascript"></script>


 @*   <link href="~/Content/style.css" rel="stylesheet" type="text/css" media="all" />*@

    <style type="text/css">
        
        .lastaction
        {
            width: 30px;
        }
        .comments
        {
            width: 100px;
        }
      .editaction
        {
            width: 5px;
        }
        .deleteaction
        {
            width: 5px;
        }
        .timediff
        {
        width:35px;
        }
    </style>

  

</head>


<body>
    @using (Html.BeginForm("ProviderDebugstatus", "Providers", FormMethod.Post, null))
    {

                <div class="content-box-content">
                <div id="gridcontent">

                                 @{WebGrid grid = new WebGrid(Model, rowsPerPage: 10, ajaxUpdateContainerId: "gridcontent");
                                                                            
                                                if (grid.TotalRowCount != 0)
                                                {
                                                 
                                                 
                                                           @grid.GetHtml(tableStyle: "gridTable", headerStyle: "gridHead", rowStyle: "gridRow", alternatingRowStyle: "gridAltRow", footerStyle: "gridFooter",
                                                  columns: grid.Columns(
                                                   grid.Column("Class_name", "Class name", canSort: false, style: "comments"),
                                                   grid.Column("Method_name", "Method name", canSort: false, style: "comments"),
                                       grid.Column("Startruntime", "Start run time", style: "lastaction", canSort: false, format: @<text>@(@item.Startruntime.ToString("dd/MM/yyyy") == "01/01/0001" ? String.Empty : @item.Startruntime.ToString())</text>),
grid.Column("Endruntime", "End run time", style: "lastaction", canSort: false, format: @<text>@(@item.Endruntime.ToString("dd/MM/yyyy") == "01/01/0001" ? String.Empty : @item.Endruntime.ToString())</text>),
                                      grid.Column("Time_Difference", "Time Difference(Sec)", canSort: false, style: "timediff")

               
                                                  ));
                                    <br />
                                    <br />
                                                }
                                                else
                                                {
                                    <table id="divoutmsg" width="100%">
                                        <tr class="background_color">
                                            <td align="center" valign="middle" height="25px">
                                               No status found.
                                            </td>
                                        </tr>
                                    </table>
                                                }
    }
    </div>

                         <div id="updateDialogonActions" title="Edit action">
    </div>
            
                </div>
                        <table border="0" cellpadding="5" cellspacing="0" width="100%">
                    <tr class="background_color">
                        <td width="66%">
                        </td>
                        <td align="right" width="64%">

                            &nbsp;
                           <input type="button" class="close4 button" name="Cancel" value="Cancel" />
                        </td>
                    </tr>
                </table>
    }
</body>
</html>

