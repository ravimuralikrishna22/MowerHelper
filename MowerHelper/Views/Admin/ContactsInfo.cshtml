@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
 @*   <meta name="viewport" content="width=device-width" />*@
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
         <title>Mower Helper-Find an Mower Helper</title>
    @Scripts.Render("~/ThScripts/ElectricianHelpscripts")
    @Styles.Render("~/Content/themes/base/TherpistStyles")
      @Scripts.Render("~/ThScripts/ThJquery")
    <script type="text/javascript">
        $(document).ready(function () {
            $("#btnCancel").live("click", function () {
                window.location = "../Admin/AddressInformation";
            });
        });
    </script>
</head>
<body>
    <div>
        @{Html.RenderAction("Topsection", "Tools");}
    </div>
    <div id="main-content">
        @Html.Partial("_NavigationTool")
        <div class="content-box">
            <div class="content-box-header">
                <h3>
                    Contacts information</h3>
            </div>
            <div class="clear">
            </div>
            <div class="content-box-content">
                <table class="border_style" border="0" cellpadding="6" cellspacing="1" width="100%">
                    <tr class="white_color">
                        <td align="right">
                            <strong>User name&nbsp;:&nbsp;</strong>
                        </td>
                        <td align="left" width="50%">
                            &nbsp;
                            @ViewBag.username
                        </td>
                    </tr>
                    <tr class="nav_blue_color">
                        <td align="right">
                            <strong>Address 1&nbsp;:&nbsp;</strong>
                        </td>
                        <td>
                            &nbsp;
                            @ViewBag.address1
                        </td>
                    </tr>
                    <tr class="white_color">
                        <td style="height: 15px" align="right">
                            <strong>Address 2&nbsp;:&nbsp;</strong>
                        </td>
                        <td style="height: 15px">
                            &nbsp;@ViewBag.address2
                        </td>
                    </tr>
                    <tr class="nav_blue_color">
                        <td style="height: 21px" align="right">
                            <strong>Zip code&nbsp;:&nbsp;</strong>
                        </td>
                        <td style="height: 21px">
                            &nbsp;@ViewBag.Czipcode
                        </td>
                    </tr>
                    <tr class="white_color">
                        <td style="height: 22px" align="right">
                            <strong>Country&nbsp;:&nbsp;</strong>
                        </td>
                        <td>
                            &nbsp;@ViewBag.Ccountry
                        </td>
                    </tr>
                    <tr class="nav_blue_color">
                        <td style="height: 15px" align="right">
                            <strong>State&nbsp;:&nbsp;</strong>
                        </td>
                        <td style="height: 15px">
                            &nbsp;@ViewBag.CState
                        </td>
                    </tr>
                    <tr class="white_color">
                        <td align="right">
                            <strong>City&nbsp;:&nbsp;</strong>
                        </td>
                        <td>
                            &nbsp;@ViewBag.CCity
                        </td>
                    </tr>
                    <tr class="nav_blue_color">
                        <td style="height: 20px" align="right">
                            <strong>Cell phone&nbsp;:&nbsp;</strong>
                        </td>
                        <td style="height: 20px">
                            &nbsp;@ViewBag.CCellPhone
                        </td>
                    </tr>
                    <tr id="trHPhone" class="white_color">
                        <td style="height: 21px" align="right">
                            <strong>Home phone&nbsp;:&nbsp;</strong>
                        </td>
                        <td style="height: 21px">
                            &nbsp;@ViewBag.CHomePhone
                            @*<b>Ext:@ViewBag.CHPExtension</b>*@
                        </td>
                    </tr>
                    <tr class="nav_blue_color">
                        <td align="right">
                            <strong>Work phone&nbsp;:&nbsp;</strong>
                        </td>
                        <td>
                            &nbsp;@ViewBag.CWorkPhone
                        </td>
                    </tr>
                    <tr class="white_color">
                        <td align="right">
                            <strong>Email&nbsp;:&nbsp;</strong>
                        </td>
                        <td>
                            &nbsp;@ViewBag.CEmail
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <br />
        <table border="0" cellpadding="0" cellspacing="0" width="100%">
            <tr class="background_color">
                <td align="right" style="padding: 10px">
                    <input class="button" type="button" name="btnCancel" id="btnCancel" value="Cancel" />
                </td>
            </tr>
        </table>
    </div>
    <div>
        @{Html.RenderAction("Bottomsections", "Tools");}
    </div>
</body>
     <script type="text/javascript">
         $(document).ready(function () {
             var TopIndex = '@Session["TopIndex"]';
              $("#Menu0").removeClass();
              $("#Menu1").removeClass();
              $("#Menu2").removeClass();
              $("#Menu3").addClass("active");
          });
    </script>
</html>
