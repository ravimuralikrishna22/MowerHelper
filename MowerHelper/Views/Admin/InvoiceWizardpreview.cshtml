@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
@*    <meta name="viewport" content="width=device-width" />*@
   <title>Mower Helper-Find an Mower Helper</title>
     <link href="~/Content/style.css" rel="stylesheet" type="text/css" media="all" />
    <script type="text/javascript">
        function Printpage() {
            document.getElementById('trprint').style.display = "none";
            window.print();
            document.getElementById('trprint').style.display = "";
            return false;
        }
        function cancelPage() {
            window.location = "../Admin/PracticeServicesInvoice";
            return false;
        }
    </script>
</head>
<body>
    <div id="main-content">
     <table border="0" cellpadding="0" cellspacing="0" width="100%">
                <tr id="trprint" class="background_color">
                    <td align="right">
                        <input type="button" value="Print" class="button" name="btnProceed"  onclick="return Printpage();"/>&nbsp;&nbsp;
                        <input type="button" name="btnProceedCancel" class="button" value="Close"  onclick="return cancelPage();"/>
                    </td>
                </tr>
            </table> 
            <br />
            <br />
    <table id="tblMain" style="border-width:0px;width:80%;" cellpadding="1" cellspacing="1" align="Center" border="0">
	<tbody><tr class="background_color" valign="bottom">
		<td style="width:300px;"><font size="5"><b>Mower Helper,Inc.</b></font><br>Mesa, Arizona,<br>85209</td>
        <td align="center">
						<h3>Invoice</h3>
					</td>
                    <td style="width:200px;" align="right">
						<b>Invoice Number:</b>
						<span id="lblInvoicenumber">@ViewBag.invoiceno</span>
						<br>
						<b>Invoice Date:</b>
						<span id="lblInvoicedate">@ViewBag.invoiceDate</span></td>
	</tr>
</tbody></table>
@Html.Raw(ViewBag.invoiceprview);
    </div>
</body>
</html>
