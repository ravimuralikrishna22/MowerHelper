                    //                }

                    if (document.getElementById('txtDob').value != '' && document.getElementById('txtDob').value != 'mm/dd/yyyy') {
                        //                alert("2");
                        var Dateid = document.getElementById('txtDob').value;
                        var Dat = Dateid.split("/");
                        var myDat = new Date(Dat[2], Dat[0] - 1, Dat[1])
                        var yearid = Dat[2];
                        var Monthid = Dat[0];
                        var MyId = Dat[1];
                        var CurDt = new Date()
                        var DatePat = /^(\d{1,2})\/(\d{1,2})\/(\d{4})$/;
                        var matchArray = Dateid.match(DatePat);

                        if (matchArray == null) {
