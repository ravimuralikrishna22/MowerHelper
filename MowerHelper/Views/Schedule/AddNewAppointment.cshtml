            //              else {ClearInlineAlert('lblErrDate', 'tdEDate', 'White'); }
            if (Monthid == 2 && MyId == 29 && (yearid % 4 != 0 || yearid % 100 == 0 && yearid % 400 != 0)) { alert('Please enter a valid day'); document.getElementById('txtApptDate').focus(); return false; }
